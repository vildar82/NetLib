<UserControl x:Class="WpfApp1.UserControl1" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:local="clr-namespace:WpfApp1" mc:Ignorable="d"
		d:DataContext="{d:DesignInstance local:UserControlViewModel, d:IsDesignTimeCreatable=True}" d:DesignHeight="300"
		d:DesignWidth="300">
	<Grid>
		<Button Content="Progress User Control" Command="{Binding TestProgress}" VerticalAlignment="Center" Margin="10" />
	</Grid>
</UserControl>